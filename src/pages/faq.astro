---
import LayoutPost from "../layouts/PostLayout.astro";
import faqs from "../data/faqs";
import Title from "../components/Title.astro";
import FaqItem from "../components/FaqItem.astro";
---

<LayoutPost title="FAQ">
    <section>
    <Title title="Preguntas Frecuentes" />

        {faqs.map((f) => <FaqItem item={f} />)}
    </section>
</LayoutPost>

<style>
    section {
        color: white;
        margin-left: auto;
        margin-right: auto;
    }
</style>